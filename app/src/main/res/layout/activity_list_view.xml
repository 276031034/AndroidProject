<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_list_view"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"

    tools:context="com.sh.shprojectdemo.ui.UserListActivity">
    <com.jruilibrary.widget.TemplateTitleBar
        android:id="@+id/template"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:titleText="列表"
        app:canBack="true"
        app:textColors="@color/white"
        android:elevation="8dp"
        android:background="@color/colorPrimary"
        ></com.jruilibrary.widget.TemplateTitleBar>
    <com.jruilibrary.widget.RefreshLayout

        android:id="@+id/refreshlayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.jruilibrary.widget.SideslipListView
            android:id="@+id/listview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:divider="@color/ui_black"
            android:dividerHeight="1px"></com.jruilibrary.widget.SideslipListView>
    </com.jruilibrary.widget.RefreshLayout>

</LinearLayout>
