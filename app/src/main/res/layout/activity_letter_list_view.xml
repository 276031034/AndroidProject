<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_letter_list_view"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.sh.shprojectdemo.ui.LetterListViewActivity">
    <com.jruilibrary.widget.TemplateTitleBar
        android:id="@+id/template"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:titleText="地区选择"
        app:canBack="true"
        app:textColors="@color/white"
        android:elevation="8dp"
        android:background="@color/colorPrimary"
        ></com.jruilibrary.widget.TemplateTitleBar>

    <com.jruilibrary.widget.UISearchView
        android:id="@+id/search_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </com.jruilibrary.widget.UISearchView>
    <com.jruilibrary.widget.letterlist.LetterListView
        android:id="@+id/listview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        ></com.jruilibrary.widget.letterlist.LetterListView>
</LinearLayout>
