            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.LoginActivity" />
        <activity android:name=".ui.SettingActivity" />
        <activity android:name=".ui.TabLayoutActivity" />
        <activity android:name=".ui.TabLayout2Activity" />
        <activity android:name=".ui.VideoRecordActivity" />
        <activity android:name=".ui.MapHomeActivity" />
        <activity android:name=".ui.RadarActivity" />
        <activity android:name=".ui.NavigationActivity" />
        <activity android:name=".ui.BNDemoGuideActivity" />
        <activity android:name=".ui.MapActivity" />
        <activity
            android:name=".ui.ShVideoActivity"
            android:configChanges="orientation|keyboardHidden|screenSize" />
        <activity android:name=".ui.LayerListViewActivity" />
        <activity android:name=".ui.AudioRecordActivity" />
        <activity android:name=".ui.MaterialDesignActivity" />
        <activity android:name=".ui.KActivity"></activity>
    </application>

</manifest>